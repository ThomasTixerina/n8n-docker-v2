# MiConsul Legacy API - Environment Configuration
# Copy this file to .env and configure your values

# Database Configuration
DB_HOST=localhost
DB_PORT=3306
DB_NAME=miconsul
DB_USER=root
DB_PASS=your_password_here
DB_CHARSET=utf8mb4

# n8n Integration
N8N_WEBHOOK_URL=https://your-n8n-url.trycloudflare.com/webhook
N8N_API_TOKEN=your_secure_api_token_here

# Security
RATE_LIMIT_REQUESTS=100
RATE_LIMIT_WINDOW=60

# Logging
LOG_LEVEL=info
LOG_PATH=/var/log/miconsul
